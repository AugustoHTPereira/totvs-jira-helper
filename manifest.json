{
    "manifest_version": 3,
    "name": "DOM Viewer",
    "version": "1.0",
    "description": "Exibe o valor de um elemento do DOM por ID",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}